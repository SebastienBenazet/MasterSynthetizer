            android:layout_height="match_parent"
            android:layout_marginBottom="1dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/heenok_brla_button"
                android:layout_width="0dp"
                android:layout_height="match_parent"
